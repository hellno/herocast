{
  "name": "tauri-boilerplate",
  "version": "0.0.1",
  "license": "",
  "private": true,
  "scripts": {
    "dev": "tauri dev",
    "start": "vite",
    "rls": "tauri build",
    "rls:debug": "tauri build --debug",
    "rls:mac": "tauri build --target universal-apple-darwin",
    "build": "cross-env INLINE_RUNTIME_CHUNK=true NODE_OPTIONS=--max-old-space-size=16192 yarn vite build",
    "preview": "vite preview",
    "serve": "serve -s build",
    "preinstall": "yarn global add serve"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@aptabase/tauri": "^0.4.1",
    "@chakra-ui/icons": "^2.0.9",
    "@chakra-ui/react": "^2.8.0",
