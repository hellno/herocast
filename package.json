    "@types/node": "^18.7.15",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^1.0.7",
    "autoprefixer": "^10.4.14",
    "buffer": "^6.0.3",
    "cross-env": "^7.0.3",
